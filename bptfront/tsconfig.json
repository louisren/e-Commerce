{
	"compilerOptions": {
		"module": "none",
		"target": "esnext",
		"sourceMap": true,
		"noEmit": false,
		"downlevelIteration": true,
		"checkJs": false,
		"allowJs": false,
		"lib": ["dom", "es5", "es2016", "esnext", "es2020"],
		"types": ["@openui5/ts-types"]
	},
	"include": ["./webapp/**/*.ts", "./typings/**/*"],
	"exclude": ["./node_modules/**/*"]
}
